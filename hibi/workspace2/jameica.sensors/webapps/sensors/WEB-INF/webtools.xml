<?xml version="1.0" encoding="ISO-8859-1"?>

<webtools xmlns:wt="http://www.willuhn.de/schema/jameica-webtools"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://www.willuhn.de/schema/jameica-webtools http://www.willuhn.de/schema/jameica-webtools-1.0.xsd ">

  <pages>
    <page pattern="^/charts" template="charts.html" controller="de.willuhn.jameica.sensors.web.controller.Charts" />
    <page pattern="^/config" template="config.html" controller="de.willuhn.jameica.sensors.web.controller.Config" />
    <page pattern=".*"       template="index.html"  controller="de.willuhn.jameica.sensors.web.controller.Status" />
  </pages>
</webtools>
